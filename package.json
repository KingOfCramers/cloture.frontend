{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.18",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-big-calendar": "^0.31.0",
    "@types/react-calendar": "^3.1.4",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "moment": "^2.29.1",
    "node-sass": "^6.0.0",
    "query-string": "^7.0.0",
    "react": "17.0.2",
    "react-big-calendar": "^0.33.2",
    "react-calendar": "^3.4.0",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "react-waypoint": "^10.1.0",
    "typescript": "^4.2.4",
    "use-query-params": "^1.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "scripts": {
    "dev:start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "dev:analyze": "source-map-explorer 'build/static/js/*.js'",
    "prod:build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test:run": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "test:coverage": "SKIP_PREFLIGHT_CHECK=true react-scripts test --coverage",
    "test:view": "live-server coverage/lcov-report",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "live-server": "^1.2.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^26.5.6"
  }
}
